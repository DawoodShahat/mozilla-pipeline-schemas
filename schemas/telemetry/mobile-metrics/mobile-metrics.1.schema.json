              },
              "type": "object"
            },
            "type": "object"
          },
          "scalars": {
            "additionalProperties": {
              "type": [
                "integer",
                "string",
                "boolean"
              ]
            },
            "type": "object"
          }
        },
        "type": "object"
      },
      "type": "object"
    },
    "os": {
